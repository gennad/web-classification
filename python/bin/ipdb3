#!/Users/gennad/web-classification/python/bin/python3.4
# EASY-INSTALL-ENTRY-SCRIPT: 'ipdb==0.8','console_scripts','ipdb3'
__requires__ = 'ipdb==0.8'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('ipdb==0.8', 'console_scripts', 'ipdb3')()
    )
